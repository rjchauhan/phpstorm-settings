<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'ExampleTest.testBasicTest'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'ExampleTest.testBasicTest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'⌥⌘R' or 'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>39</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run...">
        <Item>
          <IdeaActionID>ChooseRunConfiguration</IdeaActionID>
          <ShortCut>'⌃⌥R'</ShortCut>
          <Description>Run...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'server.php'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'server.php'</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>56</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>37</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>37</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>