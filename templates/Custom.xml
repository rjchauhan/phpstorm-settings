<templateSet group="Custom">
  <template name="modal" value="@component('components.modal', ['id' =&gt; '$ID$', 'title' =&gt; '$TITLE$'])&#10;    $END$&#10;&#10;    @slot('footer')&#10;        &#10;    @endslot&#10;@endcomponent" description="Modal View Component" toReformat="false" toShortenFQNames="true">
    <variable name="ID" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="clog" value="console.log('yeah');" description="Console log for javascript" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="ddr" value="dd($response-&gt;json());" description="Die and Dump Response Json" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="mtest" value="/** @test */&#10;public function $NAME$()&#10;{&#10;    $END$&#10;}" description="Test method" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>